Config { font = "xft:VLGothic-10:bold"
       --, font = "xft:IPA UIGothic-9:bold"
       --, font = "xft:VLGothic-9:bold"
       , bgColor = "black"
       , fgColor = "grey"
       --, position = TopW L 80
       -- 1920 * 0.80 = 1536
       -- 2560 * 0.80 = 2048
       -- height: trayer(height:20)
       , position = Static { xpos = 0, ypos = 0, width = 2048, height = 20 }
       , lowerOnStart = True
       , overrideRedirect = False
       , commands = [ Run Cpu ["-p","3","-t","Cpu:<vbar><total>%","-L","15","-H","85","--normal","green","--high","red"] 10
                    --, Run Cpu ["-p","3","-L","15","-H","85","--normal","green","--high","red"] 10
                    , Run Memory ["-p","3","-t","Mem:<usedvbar><usedratio>%"] 10
                    --, Run Memory ["-p","3","-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Date "%Y/%m/%d %a %k:%M" "date" 10
                    , Run Wireless "wlp2s0"
                                   ["-p","3",
                                    "-t","ESSID:<essid>,<qualityvbar><quality>%",
                                    "-L","40","-H","70","--low","red","--normal","yellow"]
                                   10
                    , Run BatteryP ["CMB1"]
                                   ["-t","Batt:<acstatus>,<watts>W(<leftvbar><left>%)",
                                    "-L","12","-H","80","-p","3",
                                    "--low","red","--normal","yellow","--high","green",
                                    "--","-O","On","-i","Idle","-o","Off",
                                    "-L","-12","-H","-5",
                                    "-l","red","-m","yellow","-h","green"]
                                   10
                    , Run Volume "default" "Master" ["-t","Vol<status>:<volumevbar><volume>%"] 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{  %default:Master% | %battery% | %wlp2s0wi% | %cpu% | %memory% * %swap%  <fc=#ee9a00>%date%</fc>"
}
